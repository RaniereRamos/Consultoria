# Tabelas Proporcionais com 3 variáveis
tabela.freq.hospital.tempo <- table(Hospital, Q1_profissao, Q2_tempo_prof)
ftable(tabela.freq.hospital.tempo)
summary(tabela.freq.hospital.tempo)


